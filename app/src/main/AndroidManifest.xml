<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.emrhmrc.cash10">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".application.HomeActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".application.SigninGoogleActivity" />
        <activity
            android:name=".application.SigninPhoneActivity"
            android:launchMode="singleTop"
            android:windowSoftInputMode="stateVisible" />
        <activity
            android:name=".application.SideHomeMenu"
            android:launchMode="singleTop" />
        <activity
            android:name=".application.WheelActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".application.LoginActivity"
            android:launchMode="singleTask" />
        <activity
            android:name=".application.SlotGameActivty"
            android:launchMode="singleTop"></activity>
        <activity
            android:name=".application.NewSideHomeMenu"
            android:launchMode="singleTask">

        </activity>
    </application>

</manifest>
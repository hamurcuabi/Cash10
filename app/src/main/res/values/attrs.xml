<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MaskedEditText">
        <attr name="mask" format="string" />
        <attr name="allowed_chars" format="string" />
        <attr name="denied_chars" format="string" />
        <attr name="char_representation" format="string" />
        <attr name="keep_hint" format="boolean" />
    </declare-styleable>
    <declare-styleable name="MyTextView">
        <attr name="font" format="reference" />
    </declare-styleable>

    <declare-styleable name="LuckyWheelView">
        <attr name="lkwBackgroundColor" format="color" />
        <attr name="lkwCursor" format="reference" />
        <attr name="lkwCenterImage" format="reference" />
        <attr name="lkwTextColor" format="color" />
    </declare-styleable>
</resources>